<%
  my $user_id = param('user');
  
  my $keys = app->dbi->model('ssh_public_key')->select(
    where => {'user.id' => $user_id},
    append => 'order by title'
  )->all;
  my $keys_str = '';
  for my $key (@$keys) {
    $keys_str .= "$key->{key}\n";
  }
  $self->render(text => $keys_str);
  return;
%>
